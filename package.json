{
  "name": "hiring-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "start": "next start",
    "vercel-build": "yarn prisma:generate && yarn migrate:deploy && next build",
